<div class="container">
  <ul>
    <div class="row">
      <div class="col-sm-7 offset-2">
        <h2 class="heading06" data-en="Order Details"></h2>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-8 offset-2">
        <% @orders.each do |order| %>
          <%= link_to(order_path(order), class: "list-group-item list-group-item-action") do %>
            <%= order.created_at%>
          <% end %>
        <% end %>
      </div>
    </div>
  </ul>
</div>